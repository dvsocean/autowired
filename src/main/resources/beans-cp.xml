<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean id="beanPath" class="com.autowired.autowired.initializers.BeanPath" autowire="constructor"/>

    <bean id="robot" class="com.autowired.autowired.implementations.RobotImpl"/>

    <bean id="dataFeed" class="com.autowired.autowired.feed.DataFeed" autowire="constructor">
        <constructor-arg value="Black on Black tesla" index="0"/>
        <constructor-arg value="December 1985" index="1"/>
    </bean>

    <bean id="droneImpl" class="com.autowired.autowired.implementations.DroneImpl"/>



</beans>